{
  "name": "william",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^14.15.5",
    "yarn": "^1.22.10"
  },
  "dependencies": {
    "@craco/craco": "^6.1.1",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@popperjs/core": "^2.9.1",
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "autoprefixer": "^9",
    "express": "^4.17.1",
    "postcss": "^7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  },
  "scripts": {
    "start": "yarn run build:tailwind && node server.js && craco start",
